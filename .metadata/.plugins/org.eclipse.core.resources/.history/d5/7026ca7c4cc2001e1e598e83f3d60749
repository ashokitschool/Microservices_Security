server:
  port: 3333
  
spring:
  application:
    name: API-Gateway
    
  cloud:
    gateway:
      routes:
      - id: welcome-api
        uri: lb://WELCOME
        predicates:
        - Path=/msg
        filters:
        - AuthenticationFilter
        
      - id: authentication-api
        uri: lb://AUTH-SERVICE
        predicates:
        - Path=/auth/**